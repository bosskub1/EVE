-- LocalScript (StarterGui)
local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- ‡∏ï‡∏≤‡∏£‡∏≤‡∏á‡πÅ‡∏°‡∏û‡πÅ‡∏•‡∏∞‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå
local MapScripts = {
    [10595058975] = "https://raw.githubusercontent.com/bosskub1/EVE/refs/heads/main/ArcaneLineage",
    [3351674303] = "https://raw.githubusercontent.com/bosskub1/EVE/refs/heads/main/DrivingEmpire",
    [662417684] = "https://raw.githubusercontent.com/bosskub1/EVE/refs/heads/main/LUCKY%20BLOCKS%20Battlegrounds",
    [3101667897] = "https://raw.githubusercontent.com/bosskub1/EVE/refs/heads/main/Legends%20Of%20Speed",
    
    -- ‡πÉ‡∏™‡πà PlaceId ‡πÅ‡∏°‡∏û‡∏¢‡πà‡∏≠‡∏¢‡∏ï‡∏£‡∏á‡∏ô‡∏µ‡πâ‡∏î‡πâ‡∏ß‡∏¢‡∏ñ‡πâ‡∏≤‡∏°‡∏µ
}

local MapNames = {
    [10595058975] = "Arcane Lineage",
    [3351674303] = "Driving Empire",
    [662417684] = "LUCKY BLOCKS Battlegrounds",
    [3101667897] = "Legends Of Speed"
}

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á ScreenGui
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = playerGui

-- Frame ‡∏´‡∏•‡∏±‡∏Å
local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 400, 0, 200)
Frame.Position = UDim2.new(0.5, -200, 0.5, -100)
Frame.BackgroundColor3 = Color3.fromRGB(25,25,25)
Frame.BorderSizePixel = 0
Frame.AnchorPoint = Vector2.new(0,0)
Frame.Active = true
Frame.Draggable = true
Frame.Parent = ScreenGui

-- ‡∏°‡∏∏‡∏°‡πÇ‡∏Ñ‡πâ‡∏á + Stroke Neon
local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0, 20)
UICorner.Parent = Frame

local UIStroke = Instance.new("UIStroke")
UIStroke.Thickness = 3
UIStroke.Color = Color3.fromRGB(0, 170, 255)
UIStroke.Parent = Frame

-- ‡πÑ‡∏ü‡∏ß‡∏¥‡πà‡∏á‡∏ö‡∏ô Stroke
spawn(function()
    while true do
        for i=0,1,0.02 do
            UIStroke.Color = Color3.fromHSV(i,1,1)
            task.wait(0.05)
        end
    end
end)

-- Title
local Title = Instance.new("TextLabel")
Title.Text = "üîÑ ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡πÅ‡∏°‡∏û..."
Title.Size = UDim2.new(1,0,1,0)
Title.BackgroundTransparency = 1
Title.TextColor3 = Color3.fromRGB(255,255,255)
Title.Font = Enum.Font.GothamBold
Title.TextScaled = true
Title.Parent = Frame

-- ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏û‡∏£‡πâ‡∏≠‡∏° Countdown 5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
local function LoadScriptCountdown(url, mapName)
    Title.Text = "üîÑ ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏Ç‡∏≠‡∏á "..mapName.."..."
    local seconds = 5
    for i = seconds,1,-1 do
        Title.Text = "üîÑ ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏Ç‡∏≠‡∏á "..mapName.."... ("..i.."s)"
        task.wait(1)
    end

    local success, err = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if success then
        Title.Text = "‚úÖ ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏Ç‡∏≠‡∏á "..mapName.." ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!"
        task.wait(1)
        Frame.Visible = false
    else
        Title.Text = "‚ùå ‡πÇ‡∏´‡∏•‡∏î‡∏•‡πâ‡∏°‡πÄ‡∏´‡∏•‡∏ß!"
    end
end

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡πÅ‡∏°‡∏û‡πÅ‡∏•‡∏∞‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÇ‡∏´‡∏•‡∏î‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
task.spawn(function()
    local placeId = game.PlaceId
    local url = MapScripts[placeId]
    local mapName = MapNames[placeId] or "Unknown Map"
    if url then
        LoadScriptCountdown(url, mapName)
    else
        Title.Text = "‚ùå ‡πÅ‡∏°‡∏û‡∏ô‡∏µ‡πâ‡πÑ‡∏°‡πà‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå\nPlaceId: "..placeId
    end
end)
